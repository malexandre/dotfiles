'.platform-darwin, .platform-darwin .workspace':
  'cmd-c':       'clipboard-history:copy'
  'cmd-shift-v': 'clipboard-history:paste'

'.platform-win32, .platform-win32 .workspace':
  'ctrl-c':       'clipboard-history:copy'
  'ctrl-shift-v': 'clipboard-history:paste'

'.platform-linux, .platform-linux .workspace':
  'ctrl-c':       'clipboard-history:copy'
  'ctrl-shift-v': 'clipboard-history:paste'
